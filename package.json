{
  "name": "clientapp",
  "version": "1.0.0",
  "description": "Client Project Update App",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve",
    "seed": "concurrently -k \"npm run build-ts\" \"node dist/services/database/seeder.js\"",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "test": "jest --colors --forceExit --verbose --detectOpenHandles",
    "test-coverage": "jest --colors --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "nodemon --inspect dist/server.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "author": "FuseMachines",
  "license": "ISC",
  "dependencies": {
    "@types/express-session": "^1.15.12",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-oauth2-client-password": "^0.1.2",
    "@types/uuid": "^3.4.4",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "lusca": "^1.6.1",
    "mongoose": "^5.5.5",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "typescript": "^3.4.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.26",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/lusca": "^1.5.0",
    "@types/mongoose": "^5.3.27",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.7",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0"
  }
}
